<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from bookstoreModelService WSDL service -->

  <service name="bookstoreModel" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://model"/>
      <parameter name="wsdlServiceElement" value="bookstoreModelService"/>
      <parameter name="schemaQualified" value="http://model"/>
      <parameter name="wsdlServicePort" value="bookstoreModel"/>
      <parameter name="className" value="model.BookstoreModelSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="bookstoreModel"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getBYBIDExternal" qname="operNS:getBYBIDExternal" xmlns:operNS="http://model" returnQName="retNS:getBYBIDExternalReturn" xmlns:retNS="http://model" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:bid" xmlns:pns="http://model" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getBYBIDExternal"/>

  </service>
</deployment>
